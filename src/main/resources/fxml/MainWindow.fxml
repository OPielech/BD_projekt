<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER" prefHeight="120.0" prefWidth="440.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainWindowController">
   <children>
      <Button fx:id="btnCustomer" mnemonicParsing="false" onAction="#btnCustomerClicked" prefHeight="80.0" prefWidth="180.0" text="CUSTOMER">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
      <Button fx:id="btnEmployee" mnemonicParsing="false" onAction="#btnEmployeeClicked" prefHeight="80.0" prefWidth="180.0" text="EMPLOYEE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Button>
   </children>
</HBox>
